{
  "name": "react-mysql-forthsocial",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Forthtilliath/react-forthsocial.git",
  "author": "Forth <33458877+Forthtilliath@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "start-client": "cd client && yarn run start",
    "start-server": "cd server && yarn run start",
    "start-cli": "yarn run start-client",
    "start-serv": "yarn run start-server",
    "start": "concurrently -k -n \"Server,Client\" -p \"[{name}]\" -c \"green,blue\" \"yarn run start-server\" \"yarn run start-client\""
  },
  "devDependencies": {
    "concurrently": "^6.2.1"
  }
}
